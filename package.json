{
  "name": "react-typescript-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@toolz/allow": "1.1.5",
    "@toolz/is-a-regular-object-react": "1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sanitize.css": "13.0.0",
    "styled-components": "5.3.9",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config configs/tools/webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config configs/tools/webpack/webpack.config.prod.js",
    "eject": "react-scripts eject",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx,jsx",
    "lint:fix": "yarn run eslint --fix src/**/*.{ts,tsx}",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "prettify:fix": "yarn run prettier -- --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "prepare:husky": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "api": "jsdoc-to-readme --src example.js",
    "toc": "markdown-toc -i EXAMPLE_README.md  --maxdepth 3",
    "doc": "npm run api && npm run toc",
    "markdown": "jsdoc2md example.js >> EXAMPLE_README.md",
    "jdoc": "jsdoc",
    "lint": "yarn eslint \"src/**/*.{js,ts,tsx,d.ts,md}\"",
    "test:run": "jest --config ./src/jest.config.js",
    "test": "yarn test:unit",
    "cy:test": "start-server-and-test start http-get://localhost:3030 && yarn cy:run",
    "build:cjs": "tsc --outDir dist/cjs --module es2020",
    "build:esm": "tsc --outDir dist/esm --module es2020",
    "build:webpack": "webpack",
    "format": "prettier --write \"**/*.{js,ts,tsx, jsx}\"",
    "prepare": "yarn build:cjs && yarn build:esm",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:coverage:off": "jest --collectCoverage=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@reduxjs/toolkit": "1.8.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "29.4.0",
    "@types/node": "^14.18.27",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.1",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "29.5.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-preset-jest": "29.5.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "cypress": "12.9.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "fontfaceobserver": "2.3.0",
    "gulp": "^4.0.2",
    "husky": "8.0.1",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "13.0.3",
    "node-plop": "0.26.3",
    "plop": "2.7.6",
    "pre-commit": "1.2.2",
    "prettier": "^2.8.7",
    "react-app-polyfill": "3.0.0",
    "react-helmet-async": "1.3.0",
    "react-is": "18.2.0",
    "react-redux": "7.2.8",
    "react-test-renderer": "18.2.0",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.2.1",
    "rimraf": "3.0.2",
    "serve": "14.2.0",
    "shelljs": "0.8.5",
    "stylelint": "14.12.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-jest": "1.0.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "4.13.1",
    "whatwg-fetch": "3.6.2"
  }
}
